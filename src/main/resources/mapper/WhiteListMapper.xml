<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bing.framework.mapper.WhiteListMapper">

    <!-- 查询所有启用的白名单记录 -->
    <select id="selectEnabledWhiteLists" resultType="com.bing.framework.entity.WhiteList">
        SELECT
            id,
            pattern,
            type,
            description,
            enabled,
            create_time,
            update_time
        FROM
            sys_white_list
        WHERE
            enabled = 1
        ORDER BY
            create_time DESC
    </select>

</mapper>